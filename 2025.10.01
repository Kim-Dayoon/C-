#include<cstdio>
int main()
{
	int n;
	int a[5000];
	scanf("%d",&n);
	a[3]=1;
	a[5]=1;
	a[4]=-1;
	for(int i=6;i<=n;i++)
	{
		if(a[i-5]==-1 && a[i-3]==-1)
		{
			a[i]=-1;
		}
		else if(a[i-5]>0)
		{
			a[i]=a[i-5]+1;
		}
		else if(a[i-3]>0)
		{
			a[i]=a[i-3]+1;
		}
	}
	printf("%d",a[n]);
	
}


#include<cstdio>
int a[1000];
int main()
{
	int n;
	a[1]=1;
	a[2]=2;
	scanf("%d",&n);
	for(int i=3;i<=n;i++)
	{
		a[i]=(a[i-1]+a[i-2])%10007;
	}
	printf("%d",a[n]);
}
