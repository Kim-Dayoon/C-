#include<iostream>
using namespace std;
int gcd(int a,int b)
{
	if(b==0)
	{
		return a;
	}
	else
	{
		return gcd(b,a%b);
	}
}
int main()
{
	int n;
	cin>>n;
	int tree[n+1];
	int distance[n];
	
	for(int i=0;i<n;i++)
	{
		cin>>tree[i];
	}
	for(int i=0;i<n-1;i++)
	{
		distance[i]=tree[i+1]-tree[i];
	}
	int g=distance[0];
	for(int i=1;i<n;i++)
	{
		g = gcd(g,distance[i]);
	}
	
	int answer = ((tree[n-1]-tree[0])/2)+1-n;
	
	cout << answer;
	
}
