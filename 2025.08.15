#include<cstdio>
#include<iostream>
#include<string>
using namespace std;
struct attendance_register
{
	int ID;
	string name;
	int at;
	int ab;
};
int main()
{
	int n;
	int m;
	char name[10];
	attendance_register a[10000];
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		char name[20];
		scanf("%d %s %d %d",&a[i].ID,name,&a[i].at,&a[i].ab);
		a[i].name = name;
	}
	scanf("%d",&m);
		
	for(int i=0;i<m;i++)
	{
		string temp;
		cin >> temp;
		int check=1;
		for(int j=0;j<n;j++)
		{
			if(a[j].name==temp)
			{
				printf("%d %d %d\n",a[j].ID,a[j].at,a[j].ab);
				check=0;
			}
			
		}
			if(check)
			{
				printf("-1\n");
			}
		
	}
	return 0;
}


#include<cstdio>
#include<iostream>
#include<string>
using namespace std;
struct book
{
	string s;
	int a,b;
};
int main()
{
	int c; 
	scanf("%d",&c);
	book d[10000];
	for(int i=0;i<c;i++)
	{
		cin>>d[i].s;
		scanf("%d %d",&d[i].a,&d[i].b);
	}
	string e;  
	cin>>e;
	for(int i=0;i<c;i++)
	{
		if(d[i].s == e) 
		{
			printf("%d %d",d[i].a,d[i].b);
			break;
		}
	}
	return 0;
}
