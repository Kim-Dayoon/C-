[색종이]_9주차 복습문제 
#include<cstdio>
int main()
{
	int a,b,c,d,e,f;
	scanf("%d %d %d %d %d %d",&a,&b,&c,&d,&e,&f);
	int ans=0;
	ans=d+e+f;
	a-=11+e;
	b-=5+d;
	if(c>0)
	{
		ans+=(c-1)+4+1;
		c%=4;
		if(c)
		{
			a-=8-c;
			b-=7-2*c;
		}
	}
	if(b>0)
	{
		ans+=(b-1)/9+1;
		b%=9;
		if(b)
		{
			a-=36-b*4;
		}
	}
	else
	{
		a+=b*4;
	}
	if(a>0)
	{
		ans+=(a-1)/36+1;
	}
	printf("%d",ans);
	return 0;
}

[줄 세우기]-10주차 복습문제
#include<cstdio>
int n,a[1000000],d[1000001];
int ans;
int main()
{
	int i;
	scanf("%d",&i);
	for(int i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	ans=n;
	for(int i=0;i<n;i++)
	{
		d[a[i]]=d[a[i]-1]+1;
			if(ans>n)
		{
			ans=n-d[a[i]];
		}
	}
	printf("%d",ans);
}

