#include<cstdio>
#include<queue>
using namespace std;
int main()
{
	int a,b;
	scanf("%d %d",&a,&b);
	queue<pair<int ,int> >q;
	q.push(make_pair(a,0));
	while(!q.empty())
	{
		int c=q.front().first;
		int  d=q.front().second;
		q.pop();
		if(c==b)
		{
			printf("%d",d);
			return 0;
		}
		if(c*2<=b)
		{
			q.push(make_pair(c*2,d+1));
		}
		if(c*10+1<=b)
		{
			q.push(make_pair(c*10+1,d+1));
		}
	}
	printf("-1");
	return 0;
}
